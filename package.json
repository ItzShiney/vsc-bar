{
  "name": "bar",
  "displayName": "Bar",
  "description": "Language support for Bar",
  "publisher": "Shiney",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
        {
            "id": "bar",
            "aliases": ["Bar", "bar"],
            "extensions": [".bar"],
            "configuration": "./language-configuration.json"
        }
    ],
    "grammars": [
        {
            "language": "bar",
            "scopeName": "source.bar",
            "path": "./syntaxes/bar.tmLanguage.json"
        }
    ]
  }
}